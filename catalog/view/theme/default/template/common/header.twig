	<html
	dir="{{ direction }}" lang="{{ lang }}"> <!--<![endif]-->
	<head>
		<meta charset="UTF-8"/>
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<title>{{ title }}</title>
		<base href="{{ base }}"/>
		<!-- // add url image product to head -->
		<meta name="robots" content="follow"/>
		<meta name="googlebot" content="index, follow, all"/>
		<meta name="language" content="pt-br"/>
		<meta name="revisit-after" content="3 days">
		<meta name="rating" content="general"/>
		<meta property="og:locale" content="pt_BR"/>
		<meta property="og:type" content="website"/>
		{% if fbMetas %}
			{% for fbMeta in fbMetas %}
				<meta property="og:image:url" content="{{ base }}image/{{ fbMeta.content }}"/>
				<meta property="og:image:type" content="image/jpeg"/>
			{% endfor %}
		{% else %}
			<meta property="og:image:url" content="{{ base }}_galerias/facebook.jpg"/>
			<meta property="og:image:type" content="image/jpeg"/>
		{% endif %}

		{% if description %}
			<meta name="description" content="{{ description }}"/>
			<meta property="og:description" content="{{ description }}"/>
		{% endif %}
		<!-- // end: add url image product to head -->
		{% if keywords %}
			<meta name="keywords" content="{{ keywords }}"/>
		{% endif %}
		 <script src="catalog/view/javascript/jquery/jquery-3.7.0.min.js" type="text/javascript"></script>
		<link
		href="catalog/view/javascript/bootstrap/css/bootstrap.min.css" rel="stylesheet" media="screen"/>
		 <script src="catalog/view/javascript/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
		<link href="catalog/view/javascript/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css"/>
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link href="https://fonts.googleapis.com/css2?family=Open+Sans:400,400i,300,700&display=swap" rel="stylesheet" type="text/css"/>
    	<link href="https://fonts.googleapis.com/css2?family=Play:wght@600;700&display=swap" rel="stylesheet">
		<link href="catalog/view/theme/default/stylesheet/stylesheet.css" rel="stylesheet">
		<link href="catalog/view/theme/default/stylesheet/bootstrap_update.css?2" rel="stylesheet">
		<link href="catalog/view/theme/default/stylesheet/template_custom.css?2{{ random() }}" rel="stylesheet">
		{% for style in styles %}
			<link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}"/>
		{% endfor %}
		{% for script in scripts %}
			 <script src="{{ script }}" type="text/javascript"></script>
		{% endfor %}
		 <script src="catalog/view/javascript/common.js?12" type="text/javascript"></script>
		{% for link in links %}
			<link href="{{ link.href }}" rel="{{ link.rel }}"/>
		{% endfor %}
		{% for analytic in analytics %}
			{{ analytic }}
		{% endfor %}

		 <script src="catalog/view/javascript/jquery/jquery.mask.js" type="text/javascript"></script>
		<script type="text/javascript">
			$(document).ready(function () {
				$(".mask-cpf, #input-custom-field1, #input-payment-custom-field1").mask("000.000.000-00", { reverse: true });
				$(".mask-cnpj, #input-custom-field2, #input-payment-custom-field2").mask("00.000.000/0000-00", { reverse: true });
				// $('input[name="custom_field[address][2]"]').mask('999999');
				// $('input[name="custom_field[account][5]"]').mask('99/99/9999');
				$("input[name='postcode']").mask('00000-000', { reverse: true, placeholder: "00000-000" });
				$('input[name="telephone"], #input-payment-telephone').mask("(99) 99999-9999");
				$('input[name="telephone"], #input-payment-telephone').on("blur", function () {
					var last = $(this).val().substr($(this).val().indexOf("-") + 1);

					if (last.length == 3) {
						var move = $(this).val().substr($(this).val().indexOf("-") - 1, 1);
						var lastfour = move + last;
						var first = $(this).val().substr(0, 9);

						$(this).val(first + '-' + lastfour);
					}
				});
			});
		</script>
		<!-- Meta Pixel Code -->
		<script>
		!function(f,b,e,v,n,t,s)
		{if(f.fbq)return;n=f.fbq=function(){n.callMethod?
		n.callMethod.apply(n,arguments):n.queue.push(arguments)};
		if(!f._fbq)f._fbq=n;n.push=n;n.loaded=!0;n.version='2.0';
		n.queue=[];t=b.createElement(e);t.async=!0;
		t.src=v;s=b.getElementsByTagName(e)[0];
		s.parentNode.insertBefore(t,s)}(window, document,'script',
		'https://connect.facebook.net/en_US/fbevents.js');
		fbq('init', '339534312013220');
		fbq('track', 'PageView');
		</script>
		<noscript><img height="1" width="1" style="display:none"
		src="https://www.facebook.com/tr?id=339534312013220&ev=PageView&noscript=1"
		/></noscript>
		<!-- End Meta Pixel Code -->
	</head>
	<body class="{{ body_class }}">
		<div id="top-menu">
			<div class="container">
				<div class="d-flex flex-wrap flex-column flex-lg-row justify-content-end">
					<ul class="top-menu-list">
						<li><a href="#">Início</a></li>
						<li><a href="{{ about_url }}">{{ about_title }}</a></li>
						<li><a href="{{ mural_url }}">Mural da Credibilidade</a></li>
						<li><a href="{{ manual_url }}">Manual de Uso</a></li>
					</ul>
				</div>
			</div>
		</div>
		<header id="main-header">
			<div class="container">
				<div class="d-flex flex-column flex-lg-row align-items-center justify-content-between gap-3">
					<div class="col-xs-12 col-md-4 col-lg-2">
						<div id="logo">
							{% if logo %}
								<a href="{{ home }}"><img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive"/></a>
							{% else %}
								<h1>
									<a href="{{ home }}">{{ name }}</a>
								</h1>
							{% endif %}
						</div>
					</div>
					<div class="col-xs-12 col-md-12 col-lg-6">{{ search }}</div>
					<div class="col-xs-12 col-md-12 col-lg-4">
						<div class="header-links mt-2 mt-md-0">
							<ul class="d-flex flex-wrap justify-content-center justify-content-lg-end flex-wrap">
								<li class="dropdown">
									<a href="{{ account }}" title="{{ welcome_user }}" class="dropdown-toggle" data-toggle="dropdown">
										<i class="fa fa-user"></i>
										{{ welcome_user }}
										<span class="caret"></span>
									</a>
									<ul class="dropdown-menu dropdown-menu-right">
										{% if logged %}
											<li>
												<a href="{{ account }}">{{ text_account }}</a>
											</li>
											<li>
												<a href="{{ order }}">{{ text_order }}</a>
											</li>
											<li>
												<a href="{{ transaction }}">{{ text_transaction }}</a>
											</li>
											<li>
												<a href="{{ download }}">{{ text_download }}</a>
											</li>
											<li>
												<a href="{{ logout }}">{{ text_logout }}</a>
											</li>
										{% else %}
											<li>
												<a href="{{ login }}">{{ text_login }}</a>
											</li>
											<li>
												<a href="{{ register }}">{{ text_register }}</a>
											</li>
										{% endif %}
									</ul>
								</li>
								<li>
									<a href="#" class="button-cart-open">
										<i class="fa fa-shopping-cart"></i> Carrinho <span id="header-cart-qty">({{ text_items }})</span>
									</a>
								</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
		{{ menu }}
		</header>
